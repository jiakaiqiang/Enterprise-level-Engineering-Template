
{
    "task-custom": {
        "name":"任务下发",
        "type":"approval",

         "defaultProps":[
            {
                "name":"任务名称",
                "type":"input",
                "defaultValue":"",
                "modelValue":"name",
                "placeholder":"请输入任务名称",
                "required":true
            },
            {
                "name":"任务描述",
                "type":"textarea",
                "defaultValue":"",
                "modelValue":"description",
                "placeholder":"请输入任务描述",
                "required":false
            },
            {
                "name":"任务类型",
                "type":"select",
                "defaultValue":"",
                "modelValue":"type",
                "placeholder":"请选择任务类型",
                "required":true,
                "options":[
                    {
                        "label":"普通任务",
                        "value":"normal"
                    },
                    {
                        "label":"子任务",
                        "value":"subtask"
                    }
                ]
            }
         ],
         "defaultData":{
            "name":"",
            "description":"",
            "type":"subtask"
         }


    },
